extends layout

block content
  h1= title
  - const image_names = ["Drone1.jpg", "drone2.jpg", "drone3.jpg", "drone4.jpg", "drone5.jpg"]
  - for(let i = 0; i < 3; i++)
    - const random_index = Math.floor(Math.random() * 5)
    img(src=`/images/${image_names[random_index]}`, alt=`${image_names[random_index]}`, height="200", width="200")